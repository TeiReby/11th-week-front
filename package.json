{
  "name": "likelion-blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.9",
    "react": "^18.2.0",
    "react-cookie": "^7.2.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ." // <--- 이 줄을 추가해주세요!
                     // 또는 "lint": "eslint src/**/*.{js,jsx,ts,tsx}" 등으로
                     // 구체적인 경로와 확장자를 지정할 수도 있습니다.
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "tailwindcss": "^3.4.1"
    // eslint가 devDependencies에 명시적으로 없어도
    // react-scripts의 의존성으로 포함되어 있어 npm run lint 시 실행 가능합니다.
    // 만약 CI 환경 등에서 eslint 명령어를 직접 찾지 못하는 경우,
    // 여기에 "eslint": "^버전" 형태로 추가 설치할 수도 있습니다.
    // 예: "eslint": "^8.0.0" (react-scripts 5.0.1은 ESLint 8.x 버전을 사용합니다)
  }
}
